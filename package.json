{
  "name": "plankton",
  "productName": "Plankton",
  "version": "1.5.4",
  "description": "A minimal editor for the Squids library",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Zachary Foutz",
    "email": "zacharyfoutz@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "plankton",
        "icon": "./Icon/icon.icns",
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "language": 1033,
            "manufacturer": "Zafuzi Industries"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./assets/dmg-background.png",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "zafuzi",
              "name": "plankton",
              "draft": true
            }
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-electronegativity",
          {
            "isSarif": true
          }
        ],
        ["@electron-forge/plugin-auto-unpack-natives"]
      ]
    }
  },
  "dependencies": {
    "codemirror": "^6.0.1",
    "ds": "^1.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "rpc": "^3.3.3",
    "serve-static": "^1.15.0",
    "sleepless": "^5.4.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.65",
    "@electron-forge/plugin-electronegativity": "^6.0.0-beta.65",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "electron": "15.5.5"
  }
}
